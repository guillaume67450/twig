{% extends 'base.html.twig' %}

{% block title %}
<title> 
    <h1>Products</h1> 
</title>
{% endblock %}

{# main content #}
{% block content %}

<div class="w-100"></div>
<div class="row">

{# for loop #}
{% for product in products %}
<div class="col-4">
    <div class="card">
    <div class="card-body">
        {{product}}
    </div>
    </div>
</div>
{% endfor %}

</div>
{% endblock %}